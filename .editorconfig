# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# Tab indent, JS style
[*]
end_of_line = lf
insert_final_newline = true
indent_size = 2
indent_style = tab

# Circle YAML files need to have spaces not tabs
[*.yml]
indent_style = space
indent_size = 2
